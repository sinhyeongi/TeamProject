<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.Java.Mapper.CouponMapper">
	<select id="Coupon_getDisprice" parameterType="String" resultType="Integer">
		select disprice from coupon where name = #{name}
	</select>
	<select id="Coupon_getDislate" parameterType="String" resultType="Integer">
		select dislate from coupon where name = #{name}
	</select>
</mapper>