<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="org.Java.Mapper.Review_RecMapper">
  <select id="getAllReviewRec" resultType="review_rec">
  	select * from review_rec;
  </select>
  <insert id="addReviewRec" parameterType="review_rec">
  	insert into review_rec values(#{no} , #{id})
  </insert>
  
  </mapper>